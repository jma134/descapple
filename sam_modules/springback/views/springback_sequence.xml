<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for springkback.order --> 
        <record id="seq_type_springback_order" model="ir.sequence.type">
            <field name="name">Springback Order</field>
            <field name="code">springback.order</field>
        </record>
        <record id="seq_springback_order" model="ir.sequence">
            <field name="name">Springback Order</field>
            <field name="code">springback.order</field>
            <field name="prefix">%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for springkback.order.npi --> 
        <record id="seq_type_springback_order_npi" model="ir.sequence.type">
            <field name="name">NPI Order</field>
            <field name="code">springback.order.npi</field>
        </record>
        <record id="seq_springback_order_npi" model="ir.sequence">
            <field name="name">NPI Order</field>
            <field name="code">springback.order.npi</field>
            <field name="prefix">%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
